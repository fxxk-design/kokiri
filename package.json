    "url": "git+https://github.com/petals-ui/kokiri.git"
  },
  "homepage": "https://github.com/petals-ui/kokiri",
  "scripts": {
    "start": "vue-cli-service serve",
    "core:build": "cd packages/core && npm run build",
    "core:patch-version": "cd packages/core && npm version patch && cd ../../",
    "core:release": "cd packages/core && npm run release",
    "el:build": "cd packages/element && npm run build",
    "el:patch-version": "cd packages/element && npm version patch && cd ../../",
    "el:release": "cd packages/element && npm run release",
    "ivu:build": "cd packages/view-ui && npm run build",
    "ivu:patch-version": "cd packages/view-ui && npm version patch && cd ../../",
    "ivu:release": "cd packages/view-ui && npm run release",
    "kokiri:build": "cd packages/kokiri && npm run build",
    "kokiri:patch-version": "cd packages/kokiri && npm version patch && cd ../../",
    "kokiri:release": "cd packages/kokiri && npm run release"
  },
  "devDependencies": {
    "@kokiri/core": "0.0.16",
    "@ntks/eslint-config": "0.0.0",
    "@ntks/stylelint-config": "0.0.5",
    "@types/node": "^13.11.1",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "axios": "^0.21.1",
    "element-ui": "^2.14.1",
    "eslint": "^7.21.0",
    "fibers": "^5.0.0",
    "kokiri": "0.0.5",
    "node-sass": "^5.0.0",
    "petals-ui": "0.0.17",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.12.0",
    "typescript": "^4.1.3",
    "view-design": "^4.7.0-beta.10",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.12",
    "vuedraggable": "^2.24.3"
  }
}
